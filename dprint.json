{
	"incremental": true,
	"lineWidth": 160,
	"useTabs": true,
	"indentWidth": 2,
	"typescript": {
		"semiColons": "always",
		"quoteStyle": "preferSingle"
	},
	"json": {},
	"markdown": {},
	"toml": {},
	"includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml}"],
	"excludes": [
		"**/node_modules",
		"**/*-lock.json",
		"**/.next",
		"**/.turbo",
		"**/dist",
		"pnpm-lock.yaml",
		"**/target",
		"**/data"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.64.2.wasm",
		"https://plugins.dprint.dev/json-0.14.1.wasm",
		"https://plugins.dprint.dev/markdown-0.12.2.wasm",
		"https://plugins.dprint.dev/toml-0.5.4.wasm",
		"https://plugins.dprint.dev/prettier-0.5.1.exe-plugin@3b0f36f27222453e24a25786969e687a3bcef29f3df00c9d7246cba9f85956d3"
	],
	"prettier": {
		"trailingComma": "all",
		"singleQuote": true,
		"proseWrap": "always"
	}
}
