{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 160,
  "newLineKind": "crlf",
  "typescript": {
    "semiColons": "always",
    "quoteStyle": "preferSingle"
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "dockerfile": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,dockerfile}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/.next",
    "**/.turbo",
    "**/dist",
    "pnpm-lock.yaml",
    "**/target",
    "**/data"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.66.0.wasm",
    "https://plugins.dprint.dev/json-0.15.0.wasm",
    "https://plugins.dprint.dev/markdown-0.13.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.2.wasm",
    "https://plugins.dprint.dev/prettier-0.6.1.exe-plugin@2d54d5f8dbdeb087762fadc4840c688327f5deab447c94b32ca3850106db9224"
  ],
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "proseWrap": "always"
  }
}
